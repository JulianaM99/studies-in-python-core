coffee = ["Café Latte", "Caffe Americano", "Espresso", "Cappuccino", "Macchiato"] #lista de cafés a serem escolhidos por indice.

choice = int(input()) #o inteiro escolhido será o indice

try:
	print(coffee[choice]) #chamando a var como índice para definir o range
	
except:
	print("Invalid number") #será executado caso o número esteja fora do range
	
finally:
	print("Have a good day") #será executado ao final, de toda forma
